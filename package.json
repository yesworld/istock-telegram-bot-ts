{
  "name": "istock-telegram-bot-ts",
  "version": "0.0.1",
  "description": "Telegram bot to search for photos from iStock in Google. This bot was developed for education purposes only!",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^7.0.0",
    "https-proxy-agent": "^2.2.1",
    "needle": "^2.3.0",
    "telegraf": "^3.29.0",
    "telegraf-ratelimit": "^2.0.0",
    "tress": "^1.1.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/dotenv": "^6.1.1",
    "@types/needle": "^2.0.4",
    "@types/node": "^11.13.4",
    "@types/tress": "^1.0.1",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "^3.4.3"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "telegram",
    "photo",
    "bot",
    "api",
    "foto",
    "istockphoto",
    "istock"
  ]
}
